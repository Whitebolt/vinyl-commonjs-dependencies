{
  "name": "vinyl-commonjs-dependencies",
  "description": "A vinyl adaptor for for commonjs dependency trees. Get all files that a script depends on.",
  "author": "Whitebolt <hello@whitebolt.net>",
  "contributors": [
    {
      "name": "Stephen Simpson",
      "email": "stephen@whitebolt.net"
    }
  ],
  "version": "0.7.9",
  "private": false,
  "keywords": [
    "gulpfriendly",
    "gulp",
    "common-js",
    "vinyl",
    "gulpfile",
    "vinyl-adapter",
    "streams",
    "require"
  ],
  "files": [
    "build/",
    "yarn.lock",
    "index.js"
  ],
  "dependencies": {
    "async-resolve": "^0.3.5",
    "babel-polyfill": "^6.26.0",
    "detective": "^4.5.0",
    "gulp": "^3.9.1",
    "lodash-provider": "^1.5.1",
    "lodash.castarray": "^4.4.0",
    "lodash.flattendeep": "^4.4.0",
    "lodash.isstring": "^4.0.1",
    "through2": "^2.0.3",
    "vinyl-file": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Whitebolt/vinyl-commonjs-dependencies.git"
  },
  "bugs": {
    "url": "https://github.com/Whitebolt/vinyl-commonjs-dependencies/issues"
  },
  "homepage": "https://github.com/Whitebolt/vinyl-commonjs-dependencies#readme",
  "engines": ">=4.0.0",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-syntax-async-generators": "^6.13.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "gulp-babel": "^7.0.0"
  },
  "peerDependencies": {
    "gulp": "^3.9.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "4.0.0"
          },
          "include": [
            "babel-plugin-transform-es2015-spread"
          ]
        }
      ]
    ],
    "plugins": [
      "transform-runtime",
      "syntax-async-functions",
      "syntax-async-generators",
      "transform-async-generator-functions",
      "transform-regenerator"
    ]
  }
}
